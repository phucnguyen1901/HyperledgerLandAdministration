            <td style="text-align:center"><%= dataProcessTransfer.value.From%></td>
            <td style="text-align:center"><%= dataProcessTransfer.value.To%></td>
            <td style="text-align:center"><%= dataProcessTransfer.value.TimeStart%></td>
            <td style="text-align:center"><%= dataProcessTransfer.value.TimeEnd%></td>
        </tbody>
    </table>
</div>

<%}else{%>
    
    <a href="/" class="btn btn-success">Có lỗi vui lòng quay lại</a>
    
<%}%>





